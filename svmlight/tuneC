c=10; 
while [ $c -lt 201 ]; 
do 
	#Run folds
	../svm_learn -c $c -t 2  C-tuning/fold0.tr C-tuning/fold0.model > C-tuning/log.txt
	../svm_classify C-tuning/fold0.tst C-tuning/fold0.model C-tuning/fold0.$c.predict > C-tuning/$c.results; 

	a=`less C-tuning/$c.results |egrep "Accuracy"|sed "s/.*set: //"|sed "s/\%.*//"`
	echo "$a (C: $c)"; 

	c=`expr $c + 10`; 
done > c.tuning

